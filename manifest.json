{
  "name": "Chrome Extension Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,

  "background": {
    "scripts": [
      "background.js"

    ],
    "persistent": false
  },
  "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "JohnnyAppsLogo.png",
            "32": "JohnnyAppsLogo.png",
            "48": "JohnnyAppsLogo.png",
            "128": "JohnnyAppsLogo.png"
        }
    },
    "icons": {
        "16": "JohnnyAppsLogo.png",
        "32": "JohnnyAppsLogo.png",
        "48": "JohnnyAppsLogo.png",
        "128": "JohnnyAppsLogo.png"
    }
}
